		<result column="COUNTRY_ID" property="COUNTRY_ID" jdbcType="DECIMAL" />
		<result column="COUNTRY_NAME" property="COUNTRY_NAME" jdbcType="VARCHAR" />
		<result column="TOTAL_ID" property="TOTAL_ID" jdbcType="DECIMAL" />
