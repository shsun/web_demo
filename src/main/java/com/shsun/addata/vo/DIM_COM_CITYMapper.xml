	<select id="retrieveAllByProvinceId" parameterType="java.lang.Integer" resultMap="BaseResultMap"> 
		<![CDATA[ SELECT * FROM DIM_COM_CITY where 1=1 AND COUNTRY_ID=1 AND PROVINCE_ID=${value} ]]>
	</select>
	
</mapper>