<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>

	<!--
	<properties url="file:////E:/repos/adstats/mybatis/mybatis.properties" />
	-->

	<classPathEntry location="mybatis-generator-1.3.2/lib/mysql-connector-java-5.1.13.jar" /> 
	<classPathEntry location="mybatis-generator-1.3.2/lib/ojdbc-6.0.jar" />
	
	<context id="DBTables" targetRuntime="MyBatis3">
		<commentGenerator>
			<property name="suppressAllComments" value="true" />
		</commentGenerator>
		
        <jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://localhost:3306/shsun_db?characterEncoding=utf8" userId="root" password="123456">
		</jdbcConnection>
        <!--
		<jdbcConnection driverClass="oracle.jdbc.driver.OracleDriver" connectionURL="jdbc:oracle:thin:@10.103.23.103:1523:test" userId="myname" password="123456">
		</jdbcConnection>
		-->
        
		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>


		<!--  -->
		<javaModelGenerator targetPackage="com.shsun.addata.vo" targetProject="./src/main/java">
			<property name="enableSubPackages" value="false" />
			<property name="trimStrings" value="false" />
		</javaModelGenerator>
		<sqlMapGenerator targetPackage="mapper" targetProject="./src/main/resources">
			<property name="enableSubPackages" value="false" />
		</sqlMapGenerator>
		<javaClientGenerator type="XMLMAPPER" targetPackage="com.shsun.addata.vo" targetProject="./src/main/java">
			<property name="enableSubPackages" value="false" />
		</javaClientGenerator>

		<!-- constans -->
		<table tableName="Channel" domainObjectName="Channel" 
				enableInsert="false" enableSelectByPrimaryKey="true" enableSelectByExample="false" enableDeleteByExample="false" enableCountByExample="false" enableUpdateByExample="false" selectByPrimaryKeyQueryId="true"  selectByExampleQueryId="true">
				<property name="useActualColumnNames" value="true" />
		</table>		
		<table tableName="Item" domainObjectName="Item" 
				enableInsert="false" enableSelectByPrimaryKey="true" enableSelectByExample="false" enableDeleteByExample="false" enableCountByExample="false" enableUpdateByExample="false" selectByPrimaryKeyQueryId="true"  selectByExampleQueryId="true">
				<property name="useActualColumnNames" value="true" />
		</table>
		<table tableName="Actor" domainObjectName="Actor" 
				enableInsert="false" enableSelectByPrimaryKey="true" enableSelectByExample="false" enableDeleteByExample="false" enableCountByExample="false" enableUpdateByExample="false" selectByPrimaryKeyQueryId="true"  selectByExampleQueryId="true">
				<property name="useActualColumnNames" value="true" />
		</table>
	</context>
</generatorConfiguration>
